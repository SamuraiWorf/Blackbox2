@using blackbox2_blink

@global_cooldown 6

@if interactBlock == true
    @player &4Cooldown is active!
    @return
@fi

@var interactBlock = true

@bypass /playsound minecraft:block.note_block.bell record @a[distance=..40] ~ ~ ~ 1 0.667
@player &bSplot!

@var pinkPad.switchState()
@var aquaPad.switchState()

@var pinkPad.updatePlacement()
@var aquaPad.updatePlacement()

@delay 5

@var interactBlock = false