@using blackbox2_vials

@if blockedVialInteract == true || exitOpen == true
    @return
@fi

@if checkSolution(vial4) == true
    @bypass /playsound minecraft:block.note_block.bit record @a[distance=..40] ~ ~ ~ 1 1
    @player &6&lGood Job!
    @var applySolution()
@else
    @player &4&lInvalid Solution
@fi