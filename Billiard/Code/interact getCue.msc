@using blackbox2_billiard

@cooldown 5

@for Int i in list::range(0, 36)
    @define Item item = player.getItem(i)

    @if item != null && item.getItemType() == "STICK" 
        @var player.setItem(i, null)
    @fi
@done

@delay 4

@console /execute in minecraft:overworld run give {{player.getName()}} stick{Tags:["cue"],display:{Name:'{"text":"Pool Cue","color":"dark_gray","bold":true}'}} 1